  "bugs": {
    "url": "https://github.com/oforfamar/plex-app/issues"
  },
  "homepage": "https://github.com/oforfamar/plex-app#readme",
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^20.1.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
